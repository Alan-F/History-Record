select count(*),Lost_tag_5_T2_4g , MONEY_20150501 from ASIAINFO_MON_QX_5_4G_TEST, ASIAINFO_MONEY_QX_5_4G where ASIAINFO_MON_QX_5_4G_TEST.user_id=ASIAINFO_MONEY_QX_5_4G.user_id group by money_20150501,Lost_tag_5_T2_4g ;

