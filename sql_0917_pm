select count(*),ROAM_STREAM_04_201502	,	ROAM_STREAM_04_201503	,	ROAM_STREAM_04_201504	,	ROAM_STREAM_04_201505	,	LOST_TAG_5_T2_4G	from ASIAINFO_MON_QX_5_4G_TEST group by		ROAM_STREAM_04_201502	,	ROAM_STREAM_04_201503	,	ROAM_STREAM_04_201504	,	ROAM_STREAM_04_201505	,	LOST_TAG_5_T2_4G		;

